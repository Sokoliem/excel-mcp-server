{
  "name": "excel-mcp-server",
  "version": "0.1.3",
  "description": "An MCP server that reads and writes spreadsheet data to MS Excel file",
  "author": "negokaz",
  "license": "MIT",
  "bin": {
    "excel-mcp-server": "dist/index.js"
  },
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('dist/index.js', '755')\"",
