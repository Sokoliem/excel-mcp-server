    "watch": "tsc --watch",
    "debug": "npx @modelcontextprotocol/inspector dist/index.js"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.5.0",
    "exceljs": "^4.4.0",
    "zod": "^3.24.2",
    "zod-to-json-schema": "^3.24.1"
  }
}
